<h1>Listing garagens</h1>

<table>
  <thead>
    <tr>
      <th>Endereco</th>
      <th>Nome</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @garagens.each do |garagem| %>
      <tr>
        <td><%= garagem.endereco_id %></td>
        <td><%= garagem.nome %></td>
        <td><%= link_to 'Show', garagem %></td>
        <td><%= link_to 'Edit', edit_garagem_path(garagem) %></td>
        <td><%= link_to 'Destroy', garagem, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Garagem', new_garagem_path %>

<h1>Listing clientes</h1>

<table>
  <thead>
    <tr>
      <th>Nome</th>
      <th>Cpf</th>
      <th>Dt nascimento</th>
      <th>Sexo</th>
      <th>Email</th>
      <th>Endereco</th>
      <th>Telefone</th>
      <th>Nr habilitacao</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @clientes.each do |cliente| %>
      <tr>
        <td><%= cliente.nome %></td>
        <td><%= cliente.cpf %></td>
        <td><%= cliente.dt_nascimento %></td>
        <td><%= cliente.sexo %></td>
        <td><%= cliente.email %></td>
        <td><%= cliente.endereco_id %></td>
        <td><%= cliente.telefone %></td>
        <td><%= cliente.nr_habilitacao %></td>
        <td><%= link_to 'Show', cliente %></td>
        <td><%= link_to 'Edit', edit_cliente_path(cliente) %></td>
        <td><%= link_to 'Destroy', cliente, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Cliente', new_cliente_path %>
